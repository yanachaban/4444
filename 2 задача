//Дано два цілих числа A і B (A < B). Знайти і вивести добуток всіх цілих чисел від A до B включно.
#include <stdio.h>

int main(void)
{
    int k = 1;
    int A = 0;
    int B = 0;
    printf("Введіть число A: ");
    scanf("%i", &A);
    do
    {

     printf("Введіть число B: ");
     scanf("%i", &B);

    } while(A > B);

    for (int i = A; i <= B; i++)
    {
        k *= i;

    }

    printf("%i", k);
}
